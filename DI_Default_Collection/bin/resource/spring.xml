<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
"https://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
	<bean id="d" class="beans.Dummy">
		<property name="fruits">
			<list>
				<value>Mango</value>
				<value>Apple</value>
				<value>Grapes</value>
			</list>
		</property>
		<property name="cricketers">
			<set>
				<value>Sachin</value>
				<value>Ganguly</value>
				<value>Sachin</value>
			</set>
		</property>
		<property name="cCaps">
		<map>
			<entry key="India" value="Delhi"></entry>
			<entry key="Srilanka" value="Columbo"></entry>
			<entry key="Nepal" value="Katnamdu"></entry>
		</map>
		</property>
	</bean>
</beans>